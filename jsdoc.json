{
  "tags": {
    "allowUnknownTags": true,
    "dictionaries": ["jsdoc"]
  },
  "source": {
    "includePattern": ".js$",
    "excludePattern": "(node_modules/|docs)"
  },
  "plugins": [
    "plugins/markdown"
  ],
  "opts": {
    "template": "node_modules/docdash",
    "encoding": "utf8",
    "destination": "docs/",
    "recurse": true,
    "verbose": true,
    "useLongnameInNav": false,
    "showInheritedInNav": true
  },
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": false
  },
  "docdash": {
    "typedefs": true,
    "search": true,
    "menu": {
      "Github Repo": {
        "href": "https://github.com/big-data-processor/task-adapter-base",
        "target": "_blank",
        "class": "menu-item",
        "id": "github-repo"
      }
    }
  }
}